{
  "packages": [
    "packages/*"
  ],
  "exact": true,
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "3.0.11",
  "command": {
    "init": {
      "exact": true
    },
    "publish": {
      "conventionalCommits": true,
      "changelogPreset": "eslint",
      "tagVersionPrefix": "v",
      "noCommitHooks": true,
      "message": "Release: %s",
      "npmClient": "yarn",
      "registry": "https://registry.npmjs.org",
      "verifyAccess": false
    }
  }
}
