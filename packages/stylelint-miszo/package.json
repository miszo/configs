{
  "name": "stylelint-miszo",
  "version": "3.0.6",
  "description": "Stylelint rules for my projects.",
  "author": "Miszo Radomski <miszo.radomski@gmail.com>",
  "homepage": "https://github.com/miszo/configs/tree/main/packages/stylelint-miszo#readme",
  "license": "MIT",
  "main": "lib/stylelint.config.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:miszo/configs.git",
  "bugs": {
    "url": "https://github.com/miszo/configs/issues"
  },
  "scripts": {
    "eslint": "eslint --ignore-path .gitignore ./ --ext .js",
    "eslint:fix": "yarn run eslint --fix",
    "eslint:json": "yarn run eslint --quiet -f json -o ./reports/eslint.json",
    "format": "yarn run prettier --write",
    "lint:staged": "lint-staged --no-stash",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "prettier:check": "yarn run prettier --list-different",
    "version": "yarn",
    "prepublishOnly": "node ./updateAppVersion.js"
  },
  "devDependencies": {
    "editorconfig": "0.15.3",
    "eslint": "7.6.0",
    "eslint-config-miszo": "3.0.6",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5"
  },
  "dependencies": {
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0"
  },
  "gitHead": "e67f90e43cf37284e2bfcc3b9ca84c4ec1bd0304"
}
