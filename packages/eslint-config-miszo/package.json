{
  "name": "eslint-config-miszo",
  "version": "3.0.21",
  "description": "ESLint rules for my projects.",
  "homepage": "https://github.com/miszo/configs/tree/main/packages/eslint-config-miszo#readme",
  "author": "Miszo Radomski <miszo.radomski@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "frontend",
    "rules"
  ],
  "files": [
    "emotion.js",
    "index.js",
    "jest.js",
    "jsdoc.js",
    "jsx-a11y.js",
    "prettier.js",
    "react.js",
    "tsdoc.js",
    "typescript.js",
    "webpack.js",
    "rules/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "git@github.com:miszo/configs.git",
  "bugs": {
    "url": "https://github.com/miszo/configs/issues"
  },
  "scripts": {
    "eslint": "eslint --ignore-path .gitignore ./ --ext .js",
    "eslint:fix": "yarn run eslint --fix",
    "eslint:json": "yarn run eslint --quiet -f json -o ./reports/eslint.json",
    "format": "yarn run prettier --write",
    "lint:staged": "lint-staged --no-stash",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "prettier:check": "yarn run prettier --list-different"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "babel-eslint": "10.1.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.0.0",
    "eslint-plugin-jsdoc": "30.4.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-optimize-regex": "1.2.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-tsdoc": "0.2.7",
    "eslint-plugin-unicorn": "21.0.0"
  },
  "devDependencies": {
    "editorconfig": "0.15.3",
    "eslint": "7.8.1",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1"
  },
  "peerDependencies": {
    "eslint": "7.x.x"
  },
  "gitHead": "e67f90e43cf37284e2bfcc3b9ca84c4ec1bd0304"
}
